import javax.swing.text.html.HTMLDocument.HTMLReader.IsindexAction;

public class InsertionSort {
	public static void main(String[] args) {
		int[] arr= {4,1, 6, 8, 9, 0, 5, 3, 2, 11, 10, 7};
		insertSort(arr);
		for(int x: arr) {
			System.out.print(x+" ");
		}
	}

	private static void insertSort(int[] arr) {
		int n = arr.length;
		for(int i =1;i<n;i++) {
			int key = arr[i];
			int j = i-1;
			while(j>=0) {
				if(arr[j] > key) {
					arr[j+1] = arr[j];
					j--;
				}
			}
			arr[j+1] = key;
			
		}
	}
	
}
